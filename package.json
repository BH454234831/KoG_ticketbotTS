{
  "name": "kog_ticketbotts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:generate:dev": "set ENV=dev && npm run db:generate",
    "db:generate:prod": "set ENV=prod && npm run db:generate",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:migrate:dev": "set ENV=dev && npm run db:migrate",
    "db:migrate:prod": "set ENV=prod && npm run db:migrate",
    "db:push": "drizzle-kit push --config=./drizzle.config.ts",
    "db:push:dev": "set ENV=dev && npm run db:push",
    "db:push:prod": "set ENV=prod && npm run db:push",
    "db:sync": "npm run db:generate && npm run db:push",
    "db:sync:dev": "set ENV=dev && npm run db:push",
    "db:sync:prod": "set ENV=prod && npm run db:push",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "tsx --trace-uncaught src/index.ts",
    "start:dev": "set NODE_ENV=dev && npm run start",
    "start:prod": "set NODE_ENV=prod && npm run start",
    "start:watch": "tsx watch --trace-uncaught src/index.ts",
    "start:watch:dev": "set NODE_ENV=dev && npm run start:watch",
    "start:watch:prod": "set NODE_ENV=prod && npm run start:watch",
    "build": "rimraf dist && tsc",
    "docker": "dotenv-load docker compose up -d"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/utilities": "^7.0.3",
    "discord-html-transcripts": "^3.2.0",
    "discord.js": "^14.16.3",
    "discordx": "^11.12.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "drizzle-zod": "^0.5.1",
    "file-type": "^19.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "dotenv-load": "^3.0.0",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
